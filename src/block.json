{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "mah-asides/mah-asides",
	"version": "0.1.0",
	"title": "Mah Asides",
	"category": "text",
	"icon": "smiley",
	"description": "A block to display aside information",
	"supports": {
		"html": false,
		"align": [ "wide" ]
	},
	"attributes": {
		"type": {
			"type": "string",
			"default": "note",
			"enum": [ "note", "tip", "warning" ]
		},
		"content": {
			"type": "string"
		}
	},
	"textdomain": "mah-asides",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
